    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >binomial tau-leap spatial stochastic simulation algorithm</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#KISAO_0000096">
