      <owl:Class rdf:about="#KISAO_0000025"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="KISAO_0000042"/>
    </rdfs:subClassOf>
    <oboInOwl:hasDefinition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Gillespie-like algorithm that produces exact realisation of the vector of entity amounts at each time by choosing rigorously time and reaction at each iteration.</oboInOwl:hasDefinition>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Gillespie-like exact stochastic simulation method</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#KISAO_0000002">
