    <Class rdf:about="&kisao;KISAO_0000068">
        <rdfs:label rdf:datatype="&xsd;string">deterministic cellular automata update algorithm</rdfs:label>
        <rdfs:subClassOf rdf:resource="&kisao;KISAO_0000264"/>
