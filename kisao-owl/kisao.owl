        <oboInOwl:Synonym rdf:datatype="&xsd;string">Livermore solver for ordinary differential equations, implicit sparse version</oboInOwl:Synonym>
        <oboInOwl:CreationDate>30OCT2007</oboInOwl:CreationDate>
        <rdfs:seeAlso>http://www.nea.fr/abs/html/uscd1225.html</rdfs:seeAlso>
