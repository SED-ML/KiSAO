    <!-- http://www.biomodels.net/kisao/KISAO#KISAO_0000029 -->

    <Class rdf:about="&kisao;KISAO_0000029">
        <rdfs:label rdf:datatype="&xsd;string">Gillespie&#39;s direct method</rdfs:label>
        <rdfs:subClassOf rdf:resource="&kisao;KISAO_0000241"/>
