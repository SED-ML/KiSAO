    <!-- http://www.biomodels.net/kisao/KISAO#KISAO_0000030 -->

    <Class rdf:about="&kisao;KISAO_0000030">
        <rdfs:label rdf:datatype="&xsd;string">Euler forward method</rdfs:label>
        <rdfs:subClassOf rdf:resource="&kisao;KISAO_0000261"/>
        <rdfs:subClassOf>
            <Restriction>
                <onProperty rdf:resource="&kisao;KISAO_0000245"/>
