    <!-- http://www.biomodels.net/kisao/KISAO#KISAO_0000025 -->

    <Class rdf:about="&kisao;KISAO_0000025">
        <rdfs:label rdf:datatype="&xsd;string">Gillespie-like stochastic simulation method</rdfs:label>
