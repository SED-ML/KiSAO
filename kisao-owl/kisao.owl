    <Class rdf:about="&kisao;KISAO_0000057">
        <rdfs:label rdf:datatype="&xsd;string">Brownian diffusion Smoluchowski method</rdfs:label>
        <rdfs:subClassOf rdf:resource="&kisao;KISAO_0000056"/>
        <rdfs:subClassOf>
